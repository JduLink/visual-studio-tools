<Window x:Class="Salesforce.VisualStudio.Services.ConnectedService.Views.AuthenticateRedirectHost"
        x:ClassModifier="internal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Logon"
        Width="700"
        Height="700"
        ResizeMode="NoResize"
        ShowInTaskbar="False"
        WindowStartupLocation="CenterOwner">
    <Grid>
        <Border Margin="1,1,1,1" BorderThickness="1,1,1,1">
            <WebBrowser x:Name="browser" Margin="0,0,0,0" Cursor="Arrow"/>
        </Border>
    </Grid>
</Window>
